      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import os\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Get a list of all CSV files in the current directory\n",
        "csv_files = [f for f in os.listdir('.') if f.endswith('.csv')]\n",
        "\n",
        "# Dictionary to store emissions trends for each country\n",
        "emission_trends = {}\n",
        "\n",
        "for csv_file in csv_files:\n",
        "    try:\n",
        "        # Extract country name from the filename\n",
        "        country_name = csv_file.replace('.csv', '')\n",
        "\n",
        "        # Read the CSV file\n",
        "        df = pd.read_csv(csv_file)\n",
        "\n",
        "        # Ensure the required columns are present\n",
        "        if 'Year' in df.columns and 'Emissions' in df.columns:\n",
        "            # Group by year and calculate the average emissions for each year\n",
        "            yearly_avg = df.groupby('Year')['Emissions'].mean()\n",
        "            # Store the trend for this country\n",
        "            emission_trends[country_name] = yearly_avg\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"Error processing {csv_file}: {e}\")\n",
        "\n",
        "# Calculate total emissions for each country\n",
        "total_emissions = {country: trend.sum() for country, trend in emission_trends.items()}\n",
        "\n",
        "# Create a DataFrame for total emissions\n",
        "emissions_df = pd.DataFrame(list(total_emissions.items()), columns=['Country', 'Total Emissions'])\n",
        "\n",
        "# Sort the DataFrame by total emissions in descending order\n",
        "emissions_df = emissions_df.sort_values(by='Total Emissions', ascending=False)\n",
        "\n",
        "# Select only the top 10 countries\n",
        "top_10_emissions_df = emissions_df.head(10)\n",
        "\n",
        "# Calculate cumulative percentage\n",
        "top_10_emissions_df['Cumulative'] = top_10_emissions_df['Total Emissions'].cumsum()\n",
        "top_10_emissions_df['Cumulative Percentage'] = 100 * top_10_emissions_df['Cumulative'] / top_10_emissions_df['Total Emissions'].sum()\n",
        "\n",
        "# Create the Pareto chart\n",
        "fig, ax1 = plt.subplots(figsize=(12, 6))\n",
        "\n",
        "# Bar plot for total emissions\n",
        "ax1.bar(top_10_emissions_df['Country'], top_10_emissions_df['Total Emissions'], color='skyblue', label='Total Emissions')\n",
        "ax1.set_xlabel('Country')\n",
        "ax1.set_ylabel('Total Emissions', color='skyblue')\n",
        "ax1.tick_params(axis='y', labelcolor='skyblue')\n",
        "ax1.set_xticklabels(top_10_emissions_df['Country'], rotation=45, ha='right')\n",
        "\n",
        "# Create a second y-axis for the cumulative percentage\n",
        "ax2 = ax1.twinx()\n",
        "ax2.plot(top_10_emissions_df['Country'], top_10_emissions_df['Cumulative Percentage'], color='orange', marker='o', label='Cumulative Percentage')\n",
        "ax2.set_ylabel('Cumulative Percentage (%)', color='orange')\n",
        "ax2.tick_params(axis='y', labelcolor='orange')\n",
        "\n",
        "# Add grid and titles\n",
        "ax1.grid(axis='y', linestyle='--', alpha=0.7)\n",
        "plt.title('Pareto Chart of Total Greenhouse Gas Emissions by Top 10 Countries')\n",
        "ax1.axhline(80, color='gray', linestyle='--')  # Reference line at 80% for the Pareto principle\n",
        "\n",
        "# Show the plot\n",
        "fig.tight_layout()  # Adjust layout to prevent clipping\n",
        "plt.show()\n"
      ],
