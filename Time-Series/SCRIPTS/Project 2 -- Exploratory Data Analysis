      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import os\n",
        "import plotly.graph_objects as go\n",
        "\n",
        "# Path to the directory containing the CSV files\n",
        "csv_folder_path = '/content'\n",
        "\n",
        "# Initialize a dictionary to store emissions trends for each country\n",
        "emission_trends = {}\n",
        "\n",
        "# Load each CSV file into a DataFrame and process emissions trends\n",
        "for filename in os.listdir(csv_folder_path):\n",
        "    if filename.endswith('.csv'):\n",
        "        try:\n",
        "            # Extract country name from the filename (assuming it's the part before \".csv\")\n",
        "            country_name = filename.replace('.csv', '').replace('_', ' ')\n",
        "\n",
        "            # Read the CSV file\n",
        "            df = pd.read_csv(os.path.join(csv_folder_path, filename))\n",
        "\n",
        "            # Ensure 'Year' and 'Emissions' columns exist\n",
        "            if 'Year' in df.columns and 'Emissions' in df.columns:\n",
        "                # Group by year and calculate the average emissions for each year\n",
        "                yearly_avg = df.groupby('Year')['Emissions'].mean()\n",
        "\n",
        "                # Store the trend for this country\n",
        "                emission_trends[country_name] = yearly_avg\n",
        "\n",
        "        except Exception as e:\n",
        "            print(f\"Error processing {filename}: {e}\")\n",
        "\n",
        "# Calculate the overall average emissions for each country\n",
        "average_emissions = {country: trend.mean() for country, trend in emission_trends.items()}\n",
        "\n",
        "# Sort countries by average emissions in descending order\n",
        "sorted_countries = sorted(average_emissions.items(), key=lambda item: item[1], reverse=True)\n",
        "\n",
        "# Select only the top 5 countries with the highest average emissions\n",
        "top_5_countries = sorted_countries[:5]\n",
        "\n",
        "# Create the interactive figure\n",
        "fig = go.Figure()\n",
        "\n",
        "for country, _ in top_5_countries:\n",
        "    trend = emission_trends[country]\n",
        "    fig.add_trace(go.Scatter(\n",
        "        x=trend.index,\n",
        "        y=trend.values,\n",
        "        mode='lines',\n",
        "        name=country,  # Country name will appear in the legend\n",
        "        hovertext=[country] * len(trend),  # Pass the country name to each point\n",
        "        hovertemplate=(\n",
        "            '<b>%{hovertext}</b><br>'  # Use hovertext to display the country name\n",
        "            'Year: %{x}<br>'  # Display the year (x-axis)\n",
        "            'Emissions: %{y}<br>'  # Display the emissions (y-axis)\n",
        "            '<extra></extra>'  # No additional info\n",
        "        )\n",
        "    ))\n",
        "\n",
        "# Customize layout\n",
        "fig.update_layout(\n",
        "    title=\"Greenhouse Gas Emission Trends by Top 5 Countries (Highest to Lowest Average Emissions)\",\n",
        "    xaxis_title=\"Year\",\n",
        "    yaxis_title=\"Average Greenhouse Gas Emissions\",\n",
        "    hovermode='x unified',  # Show hover data for all lines at the same x-axis point\n",
        "    legend_title=\"Top 5 Countries\",\n",
        "    template=\"plotly_white\"\n",
        ")\n",
        "\n",
        "# Show the plot\n",
        "fig.show()\n",
        "\n"
      ],
