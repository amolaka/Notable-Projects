        "# prompt: Generate a line graph to see which year had the highest emissions across all countries\n",
        "\n",
        "# Group data by year and sum emissions for all countries\n",
        "total_emissions_by_year = all_data.groupby('Year')['Emissions'].sum()\n",
        "\n",
        "# Create a line graph\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.plot(total_emissions_by_year.index, total_emissions_by_year.values, marker='o')\n",
        "plt.title('Total GHG Emissions Across All Countries')\n",
        "plt.xlabel('Year')\n",
        "plt.ylabel('Total Emissions (units)')\n",
        "plt.grid(True)\n",
        "plt.show()"
