        "import pandas as pd\n",
        "import os\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Path to the directory containing the CSV files\n",
        "csv_folder_path = '/content'\n",
        "\n",
        "# Initialize a list to hold DataFrames\n",
        "dataframes = []\n",
        "\n",
        "# Load each CSV file into a DataFrame and append to the list\n",
        "for filename in os.listdir(csv_folder_path):\n",
        "    if filename.endswith('.csv'):\n",
        "        country_data = pd.read_csv(os.path.join(csv_folder_path, filename))\n",
        "        # Add a new column for the country name based on the filename\n",
        "        country_name = filename.replace('.csv', '').replace('_', ' ')\n",
        "        country_data['Country'] = country_name\n",
        "        dataframes.append(country_data)\n",
        "\n",
        "# Concatenate all DataFrames into a single DataFrame\n",
        "all_data = pd.concat(dataframes)\n",
        "\n",
        "# Calculate the average emissions per country\n",
        "average_emissions = all_data.groupby('Country')['Emissions'].mean().reset_index()\n",
        "\n",
        "# Sort the DataFrame by average emissions in descending order\n",
        "average_emissions = average_emissions.sort_values(by='Emissions', ascending=False)\n",
        "\n",
        "# Select the top 20 emitting countries\n",
        "top_20_emissions = average_emissions.head(20)\n",
        "\n",
        "# Create a bar plot to visualize average emissions by country\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.bar(top_20_emissions['Country'], top_20_emissions['Emissions'], color='skyblue')\n",
        "plt.xticks(rotation=45, ha='right')\n",
        "plt.title('Top 20 Countries by Average Annual GHG Emissions')\n",
        "plt.xlabel('Country')\n",
        "plt.ylabel('Average Emissions (units)')\n",
        "plt.grid(axis='y', linestyle='--', alpha=0.7)\n",
        "plt.tight_layout()  # Adjust layout to prevent clipping\n",
        "plt.show()"
      ]
