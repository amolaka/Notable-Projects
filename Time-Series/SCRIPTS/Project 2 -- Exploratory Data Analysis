      "source": [
        "\n",
        "import pandas as pd\n",
        "import os\n",
        "import matplotlib.pyplot as plt\n",
        "import ipywidgets as widgets\n",
        "from IPython.display import display, clear_output\n",
        "\n",
        "# Path to the directory containing the CSV files\n",
        "csv_folder_path = '/content'\n",
        "\n",
        "# Initialize a list to hold DataFrames\n",
        "dataframes = []\n",
        "\n",
        "# Load each CSV file into a DataFrame and append to the list\n",
        "for filename in os.listdir(csv_folder_path):\n",
        "    if filename.endswith('.csv'):\n",
        "        country_data = pd.read_csv(os.path.join(csv_folder_path, filename))\n",
        "        # Add a new column for the country name based on the filename\n",
        "        country_name = filename.replace('.csv', '').replace('_', ' ')\n",
        "        country_data['Country'] = country_name\n",
        "        dataframes.append(country_data)\n",
        "\n",
        "# Concatenate all DataFrames into a single DataFrame\n",
        "all_data = pd.concat(dataframes)\n",
        "\n",
        "# Create a dropdown widget for selecting a country\n",
        "country_dropdown = widgets.Dropdown(\n",
        "    options=sorted(all_data['Country'].unique()),\n",
        "    value=all_data['Country'].unique()[0],\n",
        "    description='Country:'\n",
        ")\n",
        "\n",
        "# Create an output widget to display the plot\n",
        "output = widgets.Output()\n",
        "\n",
        "def plot_emissions_for_country(country):\n",
        "    with output:\n",
        "        clear_output(wait=True)\n",
        "        country_emissions = all_data[all_data['Country'] == country]\n",
        "        plt.figure(figsize=(10, 6))\n",
        "        plt.plot(country_emissions['Year'], country_emissions['Emissions'], marker='o')\n",
        "        plt.title(f'Carbon Emissions Trend for {country}')\n",
        "        plt.xlabel('Year')\n",
        "        plt.ylabel('Emissions (units)')\n",
        "        plt.grid(True)\n",
        "        plt.show()\n",
        "\n",
        "# Link the dropdown widget to the plot function\n",
        "widgets.interactive(plot_emissions_for_country, country=country_dropdown)\n",
        "\n",
        "# Display the dropdown and output widgets\n",
        "display(country_dropdown, output)"
