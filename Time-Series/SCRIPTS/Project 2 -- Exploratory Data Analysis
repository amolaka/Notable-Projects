      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import os\n",
        "import plotly.graph_objects as go\n",
        "import numpy as np\n",
        "\n",
        "# Get a list of all CSV files in the current directory\n",
        "csv_files = [f for f in os.listdir('.') if f.endswith('.csv')]\n",
        "\n",
        "# Dictionary to store emissions trends for each country\n",
        "emission_trends = {}\n",
        "\n",
        "for csv_file in csv_files:\n",
        "    try:\n",
        "        # Extract country name from the filename (assuming it's the part before \".csv\")\n",
        "        country_name = csv_file.replace('.csv', '')\n",
        "\n",
        "        # Read the CSV file\n",
        "        df = pd.read_csv(csv_file)\n",
        "\n",
        "        # Ensure the required columns are present\n",
        "        if 'Year' in df.columns and 'Emissions' in df.columns:\n",
        "            # Group by year and calculate the average emissions for each year\n",
        "            yearly_avg = df.groupby('Year')['Emissions'].mean()\n",
        "\n",
        "            # Store the trend for this country\n",
        "            emission_trends[country_name] = yearly_avg\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"Error processing {csv_file}: {e}\")\n",
        "\n",
        "# Calculate average emissions for all countries\n",
        "average_emissions = {country: trend.mean() for country, trend in emission_trends.items()}\n",
        "\n",
        "# Sort countries by average emissions and select the top 20\n",
        "top_20_countries = sorted(average_emissions.items(), key=lambda item: item[1], reverse=True)[:20]\n",
        "top_20_country_names = [country for country, _ in top_20_countries]\n",
        "\n",
        "# Create a DataFrame to store emissions for the top 20 countries\n",
        "all_years = sorted({year for trend in emission_trends.values() for year in trend.index})\n",
        "emissions_matrix = pd.DataFrame(index=top_20_country_names, columns=all_years)\n",
        "\n",
        "# Populate the matrix with emission data for the top 20 countries\n",
        "for country in top_20_country_names:\n",
        "    if country in emission_trends:\n",
        "        trend = emission_trends[country]\n",
        "        for year, value in trend.items():\n",
        "            emissions_matrix.loc[country, year] = value\n",
        "\n",
        "# Fill NaN values with zeros\n",
        "emissions_matrix = emissions_matrix.fillna(0)\n",
        "\n",
        "# Convert DataFrame to a NumPy array for Plotly's heatmap\n",
        "emissions_array = emissions_matrix.to_numpy()\n",
        "\n",
        "# Create the heatmap\n",
        "heatmap = go.Heatmap(\n",
        "    z=emissions_array,  # Values (emissions)\n",
        "    x=all_years,  # X-axis: years\n",
        "    y=top_20_country_names,  # Y-axis: top 20 countries\n",
        "    colorscale='Viridis',  # Color scale for heatmap\n",
        "    colorbar=dict(title=\"Emissions\", titleside='right')\n",
        ")\n",
        "\n",
        "# Create the figure with the heatmap\n",
        "fig = go.Figure(data=heatmap)\n",
        "\n",
        "# Find the last non-zero data point for each country to determine where to draw the cutoff line\n",
        "for i, country in enumerate(top_20_country_names):\n",
        "    country_data = emissions_matrix.loc[country].values\n",
        "    non_zero_years = np.where(country_data > 0)[0]\n",
        "\n",
        "    if len(non_zero_years) > 0:\n",
        "        # Find the last year where the emissions are greater than zero\n",
        "        last_non_zero_index = non_zero_years[-1]\n",
        "        cutoff_year = all_years[last_non_zero_index]\n",
        "\n",
        "        # Add a bold vertical line at the cutoff point\n",
        "        fig.add_shape(\n",
        "            type=\"line\",\n",
        "            x0=cutoff_year, x1=cutoff_year,\n",
        "            y0=i - 0.5, y1=i + 0.5,  # Draw the line at the current country row\n",
        "            line=dict(color=\"red\", width=4, dash=\"solid\"),  # Thicker and bold red cutoff line\n",
        "        )\n",
        "\n",
        "# Customize layout\n",
        "fig.update_layout(\n",
        "    title=\"Greenhouse Gas Emissions Heatmap (Top 20 Countries)\",\n",
        "    xaxis_title=\"Year\",\n",
        "    yaxis_title=\"Country\",\n",
        "    template=\"plotly_white\"\n",
        ")\n",
        "\n",
        "# Show the plot\n",
        "fig.show()\n"
      ],
