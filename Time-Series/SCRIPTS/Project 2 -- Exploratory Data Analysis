      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import os\n",
        "import plotly.express as px\n",
        "\n",
        "# Get a list of all CSV files in the current directory\n",
        "csv_files = [f for f in os.listdir('.') if f.endswith('.csv')]\n",
        "\n",
        "# Dictionary to store emissions trends for each country\n",
        "emission_trends = {}\n",
        "\n",
        "for csv_file in csv_files:\n",
        "    try:\n",
        "        # Extract country name from the filename (assuming it's the part before \".csv\")\n",
        "        country_name = csv_file.replace('.csv', '')\n",
        "\n",
        "        # Read the CSV file\n",
        "        df = pd.read_csv(csv_file)\n",
        "\n",
        "        # Ensure the required columns are present\n",
        "        if 'Year' in df.columns and 'Emissions' in df.columns:\n",
        "            # Group by year and calculate the average emissions for each year\n",
        "            yearly_avg = df.groupby('Year')['Emissions'].mean()\n",
        "\n",
        "            # Store the trend for this country\n",
        "            emission_trends[country_name] = yearly_avg\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"Error processing {csv_file}: {e}\")\n",
        "\n",
        "# Calculate average emissions for all countries\n",
        "average_emissions = {country: trend.mean() for country, trend in emission_trends.items()}\n",
        "\n",
        "# Create a DataFrame for the choropleth map\n",
        "choropleth_data = pd.DataFrame({\n",
        "    'Country': list(average_emissions.keys()),\n",
        "    'Average Emissions': list(average_emissions.values())\n",
        "})\n",
        "\n",
        "# Plotly Express choropleth map\n",
        "fig = px.choropleth(\n",
        "    choropleth_data,\n",
        "    locations='Country',  # Column with country names\n",
        "    locationmode='country names',  # Use country names\n",
        "    color='Average Emissions',  # Column with emissions values\n",
        "    hover_name='Country',  # Hover text will show country names\n",
        "    color_continuous_scale='Viridis',  # Color scale\n",
        "    title='Average Greenhouse Gas Emissions by Country',\n",
        "    labels={'Average Emissions': 'Average Emissions'},\n",
        ")\n",
        "\n",
        "# Show the choropleth map\n",
        "fig.show()\n",
        "# Print the average emissions DataFrame to check for missing countries\n",
        "print(\"Choropleth Data:\")\n",
        "print(choropleth_data)\n",
        "\n",
        "# Check which countries are not recognized by Plotly\n",
        "recognized_countries = set(px.data.gapminder().country)\n",
        "missing_countries = set(choropleth_data['Country']) - recognized_countries\n",
        "print(\"Missing Countries:\", missing_countries)\n",
        "\n",
        "# Plotly Express choropleth map\n",
        "fig = px.choropleth(\n",
        "    choropleth_data,\n",
        "    locations='Country',\n",
        "    locationmode='country names',\n",
        "    color='Average Emissions',\n",
        "    hover_name='Country',\n",
        "    color_continuous_scale='Viridis',\n",
        "    title='Average Greenhouse Gas Emissions by Country',\n",
        "    labels={'Average Emissions': 'Average Emissions'},\n",
        ")\n",
        "\n",
        "# Show the choropleth map\n",
        "fig.show()\n",
        "\n"
