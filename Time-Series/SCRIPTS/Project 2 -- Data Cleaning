        "import pandas as pd\n",
        "import os\n",
        "import shutil\n",
        "import zipfile\n",
        "\n",
        "# Load the dataset\n",
        "file_path = 'GHG_totals_by_country.csv'  # Adjust this path to the actual location of your file\n",
        "data = pd.read_csv(file_path)\n",
        "\n",
        "# Drop rows with NaN in 'Country' column (if necessary)\n",
        "data = data.dropna(subset=['Country'])\n",
        "\n",
        "# Create a new folder for the CSV files (if it doesn't exist)\n",
        "new_folder_path = '/content/csv_files'\n",
        "if not os.path.exists(new_folder_path):\n",
        "    os.makedirs(new_folder_path)\n",
        "\n",
        "# Get the list of unique countries\n",
        "countries = data['Country'].unique()\n",
        "\n",
        "# Iterate over each country\n",
        "for country in countries:\n",
        "    # Convert country to string to handle potential NaN\n",
        "    country = str(country)\n",
        "\n",
        "    # Filter the data for the current country\n",
        "    country_data = data[data['Country'] == country]\n",
        "\n",
        "    # Drop the 'Country' column from the filtered data\n",
        "    country_data = country_data.drop(columns=['Country'])\n",
        "\n",
        "    # Reshape the data: Keep 'iso', 'unit', and move all years into rows\n",
        "    reshaped_data = country_data.melt(var_name='Year', value_name='Emissions')\n",
        "\n",
        "    # Drop any null values in the emissions data\n",
        "    reshaped_data = reshaped_data.dropna(subset=['Emissions'])\n",
        "\n",
        "    # Create a clean filename (replace spaces or special characters)\n",
        "    filename = f\"{country.replace(' ', '_').replace('/', '-')}.csv\"\n",
        "\n",
        "    # Save the reshaped data to a CSV file in the new folder\n",
        "    file_path_in_folder = os.path.join(new_folder_path, filename)\n",
        "    reshaped_data.to_csv(file_path_in_folder, index=False)\n",
        "\n",
        "    print(f\"Saved: {file_path_in_folder}\")\n",
        "\n",
        "# Zip the files\n",
        "zip_file_path = '/content/csv_files.zip'\n",
        "with zipfile.ZipFile(zip_file_path, 'w') as zip_file:\n",
        "    for filename in os.listdir(new_folder_path):\n",
        "        file_path = os.path.join(new_folder_path, filename)\n",
        "        zip_file.write(file_path, arcname=filename)\n",
        "\n",
        "print(f\"Created zip file: {zip_file_path}\")\n",
        "\n"
